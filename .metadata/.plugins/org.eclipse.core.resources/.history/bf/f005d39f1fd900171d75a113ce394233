
public class YourSchedule 
{
	public static void main(String[] args)
	{
		String Eng, SW, ECS, M, SH, F;
		
		Eng= "English 11";
		SW= "Stephanie Wilson";
		ECS= "Exploring Computer Science";
		M= "Mauro";
		SH= "Stage Hand Tech";
		F= "Frazier";
		
		System.out.println("Per 1|"+ Eng +         "|"  + SW +  "|");
		System.out.println("Per 2|"+ ECS +     "|"  + M +   "|");
		System.out.println("Per 4|"+ SH +      "|"  + F +   "|");
		
		
		 
	}
	
	

}
