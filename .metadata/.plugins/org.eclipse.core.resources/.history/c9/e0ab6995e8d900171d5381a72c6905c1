import java.util.Scanner;
public class Dumbcalculator 
{
	public static void main(String[] args)
	{
		Scanner keyboard = new Scanner(System.in);
		String Fn, Sn, Tn, Av;
		
		System.out.print( "What is your First number?"); Fn = keyboard.next();
		System.out.print( "What is your Second number?"); Sn = keyboard.next();
		System.out.print( "What is your Third number?"); Fn = keyboard.next();
		Av= (Fn+Sn+Tn)/3.0;
		System.out.print( "The average of the three numbers is " + Av); 
		
	}

}
