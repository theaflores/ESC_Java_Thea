
public class UsingVariables 
{
	 public static void main(String[] args)
	 {
		 int Room;
		 double E;
		 String CS;
		 
		 
		 Room= 113;
		 E= 2.71828;
		 CS= "Computer Science";
		 
		 System.out.println( "This is room #"  + Room + ",");
		 System.out.println( "e is close to"  + E +",");
		 System.out.println( "I'm learning about" + CS + ",");
		 
				 
		 
	 }
	

}
