
public class YourInitials 
{
	

	public static void main(String[] args)
	{
		System.out.println("    FFFFFFFFFFF   M                   M");
		System.out.println("    F             M M               M M");
		System.out.println("    F             M  M             M  M");
		System.out.println("    F             M   M           M   M");
		System.out.println("    FFFFFF        M    M         M    M");
		System.out.println("    F             M     M       M     M");
		System.out.println("    F             M      M     M      M");
		System.out.println("    F             M       M   M       M");
		System.out.println("    F             M         M         M");  
		
	}
	
}
